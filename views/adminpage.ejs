<body>

    <% include paritials/navbar.ejs %>

    <div class="container" style="margin-top: 80px;">
        <div class="row">
            <div class="col-sm-4">
                <div class="card">
                    <div class="card-body text-center">
                        <h5 class="card-title text-center">User Details</h5>
                        <a href="/adminpageUser" class="btn btn-primary">Go</a>
                    </div>
                </div>
            </div>
            <div class="col-sm-4">
                <div class="card">
                    <div class="card-body text-center">
                        <h5 class="card-title text-center">Product Details</h5>
                        <a href="/adminpageProduct" class="btn btn-primary">Go</a>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" tabindex="-1" role="dialog" id="orderedModal">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header" id="orderedModal-header">
                    <h5 class="modal-title"><i class="fa fa-pagelines"></i> Organic Store</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <p id="orderedModal-content"></p>
                </div>
            </div>
        </div>
    </div>


    <% include paritials/footbar.ejs %>


    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
